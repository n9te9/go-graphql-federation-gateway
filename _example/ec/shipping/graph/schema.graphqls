extend schema
  @link(url: "https://specs.apollo.dev/federation/v2.0", import: ["@key", "@requires", "@external"])

type Query {
  shipping: String
}

type Product @key(fields: "upc") {
  upc: String! @external
  weight: Int @external
  price: Int @external
  shippingEstimate: Int @requires(fields: "weight price")
}