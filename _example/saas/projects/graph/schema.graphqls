extend schema
  @link(url: "https://specs.apollo.dev/federation/v2.0",
        import: ["@key", "@shareable"])

type Query {
  project(id: ID!): Project
}

type Project @key(fields: "id") {
  id: ID!
  name: String!
  organization: Organization!
}

extend type Organization @key(fields: "id") @shareable {
  id: ID!
}
