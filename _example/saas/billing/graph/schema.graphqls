extend schema
  @link(url: "https://specs.apollo.dev/federation/v2.0",
        import: ["@key", "@shareable", "@external", "@requires"])

type BillingInfo @key(fields: "id") {
  id: ID!
  plan: String!
}

extend type Organization @key(fields: "id") @shareable {
  id: ID!
  employeeCount: Int! @external
  billing: BillingInfo
  monthlyCost: Float! @requires(fields: "employeeCount")
}
